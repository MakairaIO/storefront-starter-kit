{
  "name": "makaira-enterprise-cloud-frontend",
  "version": "1.0.0",
  "description": "Makaira MEC Frontend",
  "author": "René Mäkeler <rm@marmalade.group>",
  "scripts": {
    "clean": "rm -f public/assets/svgs/icons.svg",
    "predev": "npm run clean",
    "dev": "npm-run-all --parallel build:svgs dev:*",
    "dev:server": "nodemon server/index.js",
    "prebuild": "npm run clean",
    "build": "next build",
    "build:svgs": "svgstore public/assets/svgs/*.svg -o public/assets/svgs/icons.svg",
    "start": "next start",
    "test": "jest --watch",
    "pre-commit": "jest --all && lint-staged"
  },
  "dependencies": {
    "@zeit/next-stylus": "^1.0.1",
    "cors": "^2.8.5",
    "dotenv-webpack": "^1.7.0",
    "express": "^4.17.1",
    "iframe-resizer": "^4.2.10",
    "isomorphic-unfetch": "^3.0.0",
    "next": "^9.2.2",
    "qs": "^6.9.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-iframe-resizer-super": "^0.2.2",
    "stylus": "^0.54.7"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.4.0",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.7",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "prettier": "^1.19.1",
    "svgstore-cli": "^1.3.1"
  },
  "pre-commit": [
    "pre-commit"
  ],
  "lint-staged": {
    "*.{js}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
